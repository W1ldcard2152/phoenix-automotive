{
  "name": "phoenix-auto",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:server": "nodemon src/server.js --watch src/api",
    "dev:client": "vite",
    "build": "vite build --emptyOutDir --outDir dist",
    "start": "node src/server.js",
    "postbuild": "node -e \"import fs from 'fs'; fs.copyFileSync('public/_redirects', 'dist/_redirects')\"",
    "start:dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "create-admin": "node scripts/createAdminUser.js",
    "cleanup": "node scripts/cleanup.js",
    "prebuild": "npm run cleanup"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.2.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "cloudinary": "^1.41.3",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "compression": "^1.8.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.263.1",
    "mongoose": "^8.1.1",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.39.0",
    "vite": "^5.0.8"
  }
}
